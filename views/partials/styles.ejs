<style>
  /* Variables */
  :root {
    --primary-color: #ffffff;
    --primary-color-dark: #000000;
    --shadow-color: rgba(0, 0, 0, 0.1);
    --accent-color: #4edcd8;
    --shadow-accent-color-light: rgba(78, 220, 216, 0.1);
    --shadow-accent-color-medium: rgba(78, 220, 216, 0.3);
    --shadow-accent-color-harsh: rgba(78, 220, 216, 0.5);
    --primary-color-rgb: 255, 255, 255;
    --filter-primary-color-dark: invert(0%) sepia(7%) saturate(29%) hue-rotate(335deg) brightness(93%) contrast(107%);
    --filter-accent-color: brightness(0) saturate(100%) invert(73%) sepia(46%) saturate(518%) hue-rotate(127deg) brightness(97%) contrast(89%);
  }

  /* Dark mode variables */
  @media (prefers-color-scheme: dark) {
    :root {
      --primary-color: #000000;
      --primary-color-dark: #ffffff;
      --shadow-color: rgba(255, 255, 255, 0.25);
      --primary-color-rgb: 0, 0, 0;
      --filter-primary-color-dark: invert(98%) sepia(98%) saturate(0%) hue-rotate(108deg) brightness(104%) contrast(101%);
    }
  }

  @media (prefers-dark-interface) {
    :root {
      --primary-color: #000000;
      --primary-color-dark: #ffffff;
      --shadow-color: rgba(255, 255, 255, 0.25);
      --primary-color-rgb: 0, 0, 0;
      --filter-primary-color-dark: invert(98%) sepia(98%) saturate(0%) hue-rotate(108deg) brightness(104%) contrast(101%);
    }
  }

  /* All text */
  #bootstrap-overrides h1,
  h2,
  h3,
  h4,
  h5,
  h6,
  p,
  a,
  a:hover,
  input {
    color: var(--primary-color-dark);
    text-decoration: none;
    margin: 0;

  }

  body {
    font-family: 'Poppins', sans-serif;
  }

  #bootstrap-overrides small {
    font-size: 0.75em;
  }

  .bodyText1 {
    font-size: 1rem;
  }

  .bodyText2 {
    font-size: 0.875rem;
  }

  .caption {
    font-size: 0.75rem;
  }

  .accent-color-text {
    color: var(--accent-color) !important;
  }

  .red-text {
    color: red !important;
  }

  .accent-color-filter {
    filter: var(--filter-accent-color);
  }

  input {
    background-color: var(--primary-color);
    caret-color: var(--accent-color);
    border: none;
    outline: none;
    box-sizing: border-box;
    transition: 0.5s;
    font-size: 14px;
    padding: 0;
    line-height: normal;
  }

  textarea {
    border: none;
    outline: none;
    box-sizing: border-box;
    background-color: var(--primary-color);
    caret-color: var(--accent-color);
    padding: 0;
    line-height: normal;
    color: var(--primary-color-dark)
  }

  ::-webkit-input-placeholder {
    line-height: inherit;
  }

  textarea:focus {
    border: none;
    outline: none;
  }

  /* All buttons */
  .btn {
    border: none;
    border-radius: 30px;
    font-weight: 400;
    font-size: 14px;
    padding: 10px 30px;
  }

  .btn:hover {
    opacity: 0.6;
  }

  /* CTA buttons */
  .btn-primary,
  .btn-primary:active,
  .btn-primary:focus {
    border: none;
    background-color: var(--accent-color);
    box-shadow: 0px 0px 24px var(--shadow-accent-color-harsh);
    color: black;
    --bs-btn-focus-shadow-rgb: none;
  }

  .btn-primary:hover {
    opacity: 1;
    background-color: var(--accent-color);
    color: black;
    transform: scale(1.03);
  }

  /* Secondary Buttons */
  .btn-secondary,
  .btn-secondary:active,
  .btn-secondary:focus {
    border: none;
    background-color: var(--primary-color);
    box-shadow: 0px 0px 24px var(--shadow-color);
    letter-spacing: 1.25px;
    color: var(--primary-color-dark);
    --bs-btn-focus-shadow-rgb: none;
    transition: 0.5s;
  }

  .btn-secondary:hover {
    background-color: var(--primary-color);
    color: var(--primary-color-dark);
    opacity: 1;
    transform: scale(1.03);
  }

  /* Disabled Buttons */
  .btn-disabled,
  .btn-disabled:hover,
  .btn-disabled:focus,
  .btn-disabled:active {
    background-color: #E0E0E0 !important;
    box-shadow: none;
  }

  /* Only Text Button */
  .btn-none,
  .btn-none:active,
  .btn-none:focus {
    border: none;
    background-color: none;
    box-shadow: none;
    letter-spacing: 1.25px;
    color: var(--primary-color-dark);
    --bs-btn-focus-shadow-rgb: none;
    transition: 0.5s;
  }

  .btn-none:hover {
    background-color: none;
    color: var(--primary-color-dark);
    opacity: 1;
    transform: scale(1.03);
  }

  .bg-black {
    background-color: black;
  }

  .bg-color {
    background-color: var(--primary-color);
  }

  .light-shadow {
    box-shadow: 0px 0px 12px var(--shadow-color);
  }

  /* Header */
  .navbar {
    padding: 1em 3em;
  }

  .navbar .show .btn,
  .navbar .collapsing .btn {
    margin-top: 0.5em;
  }

  .navbar-toggler,
  .navbar-toggler:active,
  .navbar-toggler:focus,
  .navbar-toggler-icon,
  .navbar-toggler-icon:focus {
    border: none;
    outline: none;
    box-shadow: none;
    padding: 0;
  }

  .navbar-brand:hover {
    margin-right: 1rem;
  }

  .nav-link,
  .nav-link:focus,
  .nav-link:hover {
    color: var(--primary-color-dark);
    font-weight: 500;
  }

  .nav-item {
    list-style-type: none;
  }

  .dropdown-menu-right {
    right: 0 !important;
    left: auto !important;
  }

  .dropdown-item {
    color: var(--primary-color-dark);
    font-weight: 500;
  }

  .caption {
    font-size: 0.75rem;
  }

  /* Bubbles */
  .bubble {
    border-radius: 30px;
    font-size: 0.75rem;
    padding: 0.25rem 0.75rem;
    box-shadow: 0px 0px 12px var(--shadow-color);
  }

  /* Interest Bubble */
  .interest-bubble {
    background-color: var(--primary-color);
    box-shadow: 0px 0px 12px var(--shadow-color);
    transition: 0.5s;
  }

  .interest-bubble:hover,
  .interest-highlighted {
    background-color: var(--accent-color);
    box-shadow: 0px 0px 12px var(--shadow-accent-color-light);
    color: black;
  }

  /* Personality Bubble */
  .personality-bubble {
    transition: 0.5s;
    color: black;
  }

  .recommended-personality-bubble {
    background-color: var(--accent-color);
    box-shadow: 0px 0px 12px var(--shadow-accent-color-medium);
    color: black;
  }

  .potential-personality-bubble {
    background-color: #ffd914;
    box-shadow: 0px 0px 12px rgba(255, 217, 20, 0.3);
    color: black;
  }

  .other-personality-bubble {
    background-color: #e86060;
    box-shadow: 0px 0px 12px rgba(232, 96, 96, 0.3);
    color: black;
  }

  .recommended-picture {
    box-shadow: 0px 0px 12px var(--shadow-accent-color-harsh);
  }

  .potential-picture {
    box-shadow: 0px 0px 12px rgba(255, 217, 20, 0.5);
  }

  .other-picture {
    box-shadow: 0px 0px 12px rgba(232, 96, 96, 0.5);
  }


  /* Horoscope Bubble */

  .horoscope-bubble {
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center center;
    background-image: url('/static/space.png');
    color: white;
  }

  /* Enneagram Bubble */

  .enneagram-bubble {
    background-color: #13A6A2;
    box-shadow: 0px 0px 12px rgba(19, 166, 162, 0.3);
  }

  .enneagram-bubble p {
    color: white;
    margin: 0;
  }

  .enneagram-wing {
    padding: 0px 1px 1px 1px;
  }

  /* Recommendation Bubbles */
  .recommended-bubble {
    background-color: var(--accent-color);
    color: black;
    font-size: 16px;
    padding: 0.5rem 2rem;
  }

  .potential-bubble {
    background-color: #ffd914;
    color: black;
    font-size: 16px;
    padding: 0.5rem 2rem;
  }

  .challenging-bubble {
    background-color: #e86060;
    color: black;
    font-size: 16px;
    padding: 0.5rem 2rem;
  }

  /* Circle Picture */
  .circle-picture-small {
    background-size: cover;
    background-position: center center;
    border-radius: 50%;
    height: 60px;
    width: 60px;
    object-fit: cover;
    aspect-ratio: 1/1;
  }

  .circle-picture-large {
    background-size: cover;
    background-position: center center;
    border-radius: 50%;
    max-width: 180px;
    object-fit: cover;
    aspect-ratio: 1/1;
    box-shadow: 0px 0px 40px var(--shadow-accent-color-harsh);
    display: flex;
  }

  /* Shadow Colors */
  .recommended-glow {
    box-shadow: 0px 0px 12px var(--shadow-accent-color-harsh);
  }

  /* Shadow Container */
  .shadow-container {
    box-shadow: 0px 0px 24px var(--shadow-color);
    background-color: var(--primary-color);
    border-radius: 30px;
  }

  .gold-award {
    box-shadow: 0px 0px 16px #ffd914;
  }

  .premium-award {
    box-shadow: 0px 0px 16px #ef7fef;
  }

  .blue-glow {
    box-shadow: 0px 0px 16px #4edcd8;
  }

  .red-glow {
    box-shadow: 0px 0px 16px red;
  }

  .placeholder-error::-webkit-input-placeholder {
    color: red;
  }

  .placeholder-error:-moz-placeholder {
    color: red;
  }

  .placeholder-error:-ms-input-placeholder {
    color: red;
  }

  input[type=text]::-webkit-input-placeholder {
    transition: color 0.5s;
  }

  input[type=text]:-moz-placeholder {
    transition: color 0.5s;
  }

  input[type=text]:-ms-input-placeholder {
    transition: color 0.5s;
  }

  textarea[type=text]::-webkit-input-placeholder {
    transition: color 0.5s;
  }

  textarea[type=text]:-moz-placeholder {
    transition: color 0.5s;
  }

  textarea[type=text]:-ms-input-placeholder {
    transition: color 0.5s;
  }

  /* Sort Posts */
  .dropdown-menu {
    transform: none !important;
    border-radius: 20px;
    box-shadow: 0px 0px 24px var(--shadow-color);
    background-color: var(--primary-color);
    padding: 0;
    border: none;
    background-color: var(--primary-color);
    width: 100%;
  }

  ul {
    padding: 0;
  }

  .dropdown-toggle {
    font-weight: 400;
    font-size: 14px;
    color: var(--primary-color-dark);
  }

  .dropdown-item {
    font-weight: 400;
    font-size: 14px;
    padding: 0.5rem 1rem;
    transition: 0.5s;
  }

  .dropdown-item:hover,
  .dropdown-item:focus,
  .dropdown-item:active {
    font-weight: 400;
    background-color: var(--accent-color);
    color: black;
  }

  .first-sort {
    border-radius: 20px 20px 0 0;
  }

  .last-sort {
    border-radius: 0 0 20px 20px;
  }

  .dropdown-toggle::after {
    display: none;
  }

  .dropdown-menu li {
    position: relative;
  }

  .dropdown-menu .dropdown-submenu {
    display: none;
    position: absolute;
    left: 50%;
    width: 50%;
    top: 0px;
  }

  .dropdown-menu>li:hover>.dropdown-submenu {
    display: block;
  }

  /* prompts */

  .prompt-dropdown>li:first-child>a {
    border-radius: 20px 20px 0 0;
  }

  .prompt-dropdown>li:last-child>a {
    border-radius: 0 0 20px 20px;
  }

  /* Search */

  #search ul {
    margin-bottom: 0;
  }

  /* Animations */

  .hoverable {
    transition: all .5s ease;
    backface-visibility: hidden;
    -webkit-font-smoothing: subpixel-antialiased;
  }

  .hoverable-accent-color:hover {
    color: var(--accent-color) !important;
  }

  .hoverable-filter-accent-color:hover {
    filter: var(--filter-accent-color);
  }

  .hoverable-opacity:hover {
    opacity: 0.6 !important;
  }

  .hover-harsh-glow:hover {
    box-shadow: 0px 0px 24px var(--shadow-accent-color-harsh) !important;
  }

  .hover-light-glow:hover {
    box-shadow: 0px 0px 12px var(--shadow-accent-color-light) !important;
  }

  .hover-background-color:hover {
    background-color: var(--accent-color);
    color: black;
  }

  .hover-grow-size:hover {
    transform: scale(1.01) perspective(1px) translateZ(0);
    transform-origin: 51% 51%;
  }

  .hover-grow-size-2:hover {
    transform: scale(1.02) perspective(1px) translateZ(0);
    transform-origin: 51% 51%;
  }

  .hover-grow-size-3:hover {
    transform: scale(1.03) perspective(1px) translateZ(0);
    transform-origin: 51% 51%;
  }

  .hover-grow-size-5:hover {
    transform: scale(1.05) perspective(1px) translateZ(0);
  }

  .hover-grow-size-10:hover {
    transform: scale(1.1) perspective(1px) translateZ(0);
  }

  .active-harsh-glow:focus {
    box-shadow: 0px 0px 24px var(--shadow-accent-color-harsh) !important;
  }

  .active-shrink-size:active {
    transform: scale(0.99);
  }

  .active-shrink-size-large:active {
    transform: scale(0.5)
  }

  .transition {
    transition: all 0.5s;
  }

  .adjustable-transition {
    transition: all 0.5s;
  }

  .blink {
    animation: blinker 2s linear infinite;
  }

  @keyframes blinker {
    50% {
      opacity: 0.5;
    }
  }

  /* Highlighting */
  ::selection {
    background: rgba(78, 220, 216, 0.2);
  }

  ::-moz-selection {
    background: rgba(78, 220, 216, 0.2);
  }

  /* Audio */
  audio {
    width: 100%
  }

  /* Auth Modal */
  .modal {
    background-color: rgba(0, 0, 0, 0.4);
    /* Black w/ opacity */
    z-index: 1;
  }

  .modal-backdrop {
    z-index: -1;
  }

  .modal-header {
    border: none;
  }

  .modal-header .btn-close:active,
  .modal-header .btn-close:focus {
    border: none;
    outline: none;
    box-shadow: none;
  }

  .modal-footer {
    border: none;
  }

  .modal-content {
    background-color: var(--primary-color);
    border-radius: 30px;
    height: 100%;
    box-shadow: 0px 0px 24px var(--shadow-color);
  }

  .modal-dialog {
    overflow-y: initial !important;
  }

  .modal-section {
    height: 100%;
    overflow-y: auto;
  }

  /* Add / Subtract Buttons */

  .add-button {
    background-color: var(--accent-color);
    border-radius: 5px;
    color: var(--primary-color);
    padding: 0.75rem 0.5rem;
    line-height: 0px;
    cursor: pointer;
  }

  .subtract-button {
    background-color: red;
    border-radius: 5px;
    color: var(--primary-color);
    padding: 0.75rem 0.65rem;
    line-height: 0px;
    cursor: pointer;
  }

  /* Text Decoraction */

  .underline {
    text-decoration: underline
  }

  /* Sign in and Log out Button */
  .logout-button {
    display: none;
  }

  .sign-in-button {
    display: none;
  }

  /* Date picker */
  .datepicker-picker {
    border-radius: 20px !important;
    box-shadow: 0px 0px 24px var(--shadow-color) !important;
    padding: 1rem;
  }

  .user-picture-small {
    background-size: cover;
    background-position: center center;
    border-radius: 50%;
    height: 40px;
    width: 40px;
    object-fit: cover;
    aspect-ratio: 1/1;
  }

  .share-link {
    display: none;
    position: absolute;
    border-radius: 15px;
    background-color: white;
    padding: 0.1em 1em 0.1em 1em;
    box-shadow: 0px 0px 10px rgb(0 0 0 / 20%);
    z-index: 1;
    top: -12px;
  }

  .comments-icon {
    height: 16px;
  }

  .clickable:hover {
    cursor: pointer;
  }

  .scrollbar-hidden {
    overflow-y: hidden;
  }

  .scrollbar-hidden:hover {
    overflow-y: auto;
  }


  *::-webkit-scrollbar {
    width: 6px;
    height: 6px;
  }

  *::-webkit-scrollbar-thumb {
    background-color: #cacaca;
    border-radius: 20px;
  }

  @media (pointer:none),
  (pointer:coarse) {
    .interest-bubble {
      transition: all 0.1s;
    }

    .dropdown-item {
      transition: all 0.1s;
    }

    .adjustable-transition {
      transition: all 0.1s;
    }
  }


