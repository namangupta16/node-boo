  function openDropdown(id) {
    document.getElementById("share-dropdown-" + id).classList.toggle("show");
  }

  // Close the dropdown if the user clicks outside of it
  document.addEventListener('click', function (event) {
    if (!event.target.matches('.share')) {
      var dropdowns = document.getElementsByClassName("dropdown-content");
      var i;
      for (i = 0; i < dropdowns.length; i++) {
        var openDropdown = dropdowns[i];
        if (openDropdown.classList.contains('show')) {
          openDropdown.classList.remove('show');
        }
      }
    }
  }, false);

  function copy(link) {
    try {
      navigator.clipboard.writeText(window.location.origin + link);
    } catch (e) {
      var input = document.createElement("textarea");
      input.value = window.location.origin + link;
      document.body.appendChild(input);
      input.select();
      document.execCommand("Copy");
      input.remove();
    }
  }
  function authContinue(current, next) {
    $(`#${current}`).fadeOut(500, function () {
      $(`#${current}`).attr("style", "display: none !important");
      $(`#${next}`).fadeIn(500);
    });
    document.querySelector(".current-page-value").value = next;
    document.querySelector(".current-page-button").click();
    if (current == "authQuizIntro") {
      resetQuiz(next == "authShortQuiz")
    }
    if (next == "authDescription") {
      setTimeout(function () {
        document.getElementById("descriptionInput").focus();
      }, 1000);
    }
  }

  function skip(current) {
    let user = JSON.parse(localStorage.getItem("user"));
    if (current == "authPrompts") {
      if (!user["interestNames"] || user["interestNames"].length == 0) {
        authContinue('authPrompts', 'authInterests')
      } else if (!user["languages"] || user["languages"].length == 0) {
        authContinue('authPrompts', 'authDimensions')
      } else if (!user["pictures"] || user["pictures"].length == 0) {
        authContinue('authPrompts', 'authPhotos')
      }
    } else if (current == "authInterests") {
      if (!user["languages"] || user["languages"].length == 0) {
        authContinue('authInterests', 'authDimensions')
      } else if (!user["pictures"] || user["pictures"].length == 0) {
        authContinue('authInterests', 'authPhotos')
      }
    } else if (current == "authPhotos") {
      if (!user["description"] || user["description"].length == 0) {
        authContinue('authPhotos', 'authDescription')
      } else {
        $('#authModal').modal('hide');
      }
    }
  }

  function toggleLogin() {
    loginPage = document.querySelectorAll(".login-page")
    registerPage = document.querySelectorAll(".create-account")
    loginPage.forEach(el => {
      if (el.style.display == "" || el.style.display == "block") {
        el.style.display = "none"
        registerPage.forEach(el => {
          el.style.display = "block"
        })
      } else {
        el.style.display = "block"
        registerPage.forEach(el => {
          el.style.display = "none"
        })
      }
    })
  }

  function modalNav(previous, next) {
    document.querySelectorAll(`.${previous}`).forEach(el => {
      el.style.display = "none"
    })
    document.querySelectorAll(`.${next}`).forEach(el => {
      el.style.display = "block"
      document.body.scrollTop = 0; // For Safari
      document.documentElement.scrollTop = 0; // For Chrome, Firefox, IE and Opera
    })
  }

  function checkBasicInformationCompletion() {
    let submitUserInfoBtn = document.querySelector(".submit-user-info");
    if (document.getElementById("user_firstname").value && document.getElementById("user_gender").value && document.getElementById("user_birthday").value) {
      submitUserInfoBtn.classList.remove("btn-disabled");
    } else {
      submitUserInfoBtn.classList.add("btn-disabled");
    }
  }

  function selectGender(gender) {
    document.querySelectorAll(".gender-container").forEach(el => {
      el.classList.remove("active-gender");
    })
    document.querySelectorAll(`.gender-${gender}`).forEach(el => {
      el.classList.toggle("active-gender");
    })
    document.querySelector(".user-gender").value = gender;
    checkBasicInformationCompletion();
  }

  function selectGenderFriends(gender) {
    document.querySelectorAll(`.gender-friends-${gender}`).forEach(el => {
      el.classList.toggle("active-gender");
      let prefFriendsContainer = document.getElementById(`friends-${gender}`)
      if (el.classList.contains("active-gender")) {
        prefFriendsContainer.value = gender
      } else {
        prefFriendsContainer.value = ""
      }
    })
    checkPreferenceCompletion();
  }

  function selectGenderDating(gender) {
    document.querySelectorAll(`.gender-dating-${gender}`).forEach(el => {
      el.classList.toggle("active-gender");
      let prefDatingContainer = document.getElementById(`dating-${gender}`)
      if (el.classList.contains("active-gender")) {
        prefDatingContainer.value = gender
      } else {
        prefDatingContainer.value = ""
      }
    })
    checkPreferenceCompletion();
  }

  function checkPreferenceCompletion() {
    let submitButton = document.querySelector(".submit-user-preference");
    let prefFriends = []
    let prefDating = []
    document.querySelectorAll(".pref-friends").forEach(el => {
      if (el.value != "") {
        prefFriends.push(el.value)
      }
    })
    document.querySelectorAll(".pref-dating").forEach(el => {
      if (el.value != "") {
        prefDating.push(el.value)
      }
    })
    if (prefFriends.length > 0 || prefDating.length > 0) {
      submitButton.classList.remove("btn-disabled");
    } else {
      submitButton.classList.add("btn-disabled");
    }
  }

  function selectMbti(mbti) {
    document.querySelectorAll('.mbti-type-container').forEach(el => {
      el.classList.remove("selected-active")
    })
    document.querySelector(`.mbti-${mbti}`).classList.toggle("selected-active")
    let mbtiContainer = document.getElementById("user-mbti")
    if (mbtiContainer) {
      mbtiContainer.value = mbti
    }
  }

  function closeSwiper() {
    document.querySelector(".mutation-observer-target").style.display = "none"
  }

  let questionAnswers = {}

  function answerQuestion(id, index) {
    let formatId = id.replace('q-', '')
    questionAnswers[`${formatId}`] = Number(index) / 6
    document.querySelectorAll('.' + id).forEach(el => {
      el.classList.remove("selected-question")
    });
    document.querySelector('#' + id + '-' + index).classList.toggle("selected-question");
    let submitButton = document.querySelector(".submit-accurate-questions");
    if (Object.keys(questionAnswers).length == 30) {
      submitButton.classList.remove("btn-disabled");
    } else {
      submitButton.classList.add("btn-disabled");
    }
  }

  const binaryChoiceQuestions = [
    {
      id: "pnpBs",
      mbtiCategory: "EI",
      plusEffect: true,
      question: {
        "You don't like to draw attention to yourself and keep to the background.": 0,
        "You talk to a lot of different people at parties.": 1
      }
    },
    {
      id: "HrYNF",
      mbtiCategory: "NS",
      plusEffect: true,
      question: {
        "You're a realist, you're practical and see things for what they realistically are.": 0,
        "You're a visionary, you envision and imagine what things could be even if unrealistic.": 1
      }
    },
    {
      id: "yXX0y",
      mbtiCategory: "FT",
      plusEffect: true,
      question: {
        "You dislike talking about your feelings and emotional expression.": 0,
        "If your friend is sad about something, your first instinct is to support them emotionally, not try to solve their problem.": 1
      }
    },
    {
      id: "CIsL9",
      mbtiCategory: "JP",
      plusEffect: true,
      question: {
        "You often make decisions on a whim.": 0,
        "Your personal work style is closer to organized and consistent efforts rather than spontaneous bursts of energy.": 1
      }
    },
    {
      id: "044hF",
      mbtiCategory: "TA",
      plusEffect: true,
      question: {
        "You rarely think about what you should have said in a conversation long after it has taken place.": 0,
        "You tend to start doubting yourself, your abilities, or your knowledge after making a mistake.": 1
      }
    },
  ];

  function resetQuiz(isShortQuiz) {
    questionAnswers = {}
    let ids = ["q-pnpBs", "q-HrYNF", "q-yXX0y", "q-CIsL9", "q-044hF"];
    let currentId = "q-pnpBs";
    let element;
    ids.forEach((id) => {
      if (document.getElementById(id)) {
        element = document.getElementById(id);
        currentId = id;
      };
    });
    $(`#${currentId}`).fadeOut(500, function () {
      let nextId = ids[0];
      let questions = Object.entries(binaryChoiceQuestions[0].question);
      element.id = nextId;
      let firstElement = document.getElementById(currentId + '-0');
      let secondElement = document.getElementById(currentId + '-1');

      firstElement.id = nextId + '-0';
      firstElement.classList.remove(currentId);
      firstElement.classList.remove("selected-question")
      firstElement.classList.toggle(nextId);
      if (isShortQuiz) {
        firstElement.setAttribute("onClick", "confirmShortQuestion('" + nextId + "', 0)")
      } else {
        firstElement.setAttribute("onClick", "answerQuestion('" + nextId + "', 0)")
      }
      firstElement.innerHTML = questions[0][0];

      secondElement.id = ids[0] + '-1';
      secondElement.classList.remove(currentId);
      secondElement.classList.remove("selected-question")
      secondElement.classList.toggle(nextId);
      if (isShortQuiz) {
        secondElement.setAttribute("onClick", "confirmShortQuestion('" + nextId + "', 1)")
      } else {
        secondElement.setAttribute("onClick", "answerQuestion('" + nextId + "', 1)")
      }
      secondElement.innerHTML = questions[1][0];

      $(`#${nextId}`).fadeIn(500);
    });
    document.querySelectorAll('[class^="likert-"]').forEach(el => {
      el.classList.remove("selected-question")
    });
    let submitButton = document.querySelector(".submit-accurate-questions");
    submitButton.classList.add("btn-disabled");
  }

  function confirmShortQuestion(id, index) {
    let formatId = id.replace('q-', '')
    questionAnswers[`${formatId}`] = Number(index) / 6
    document.querySelectorAll('.' + id).forEach(el => {
      el.classList.remove("selected-question")
    });
