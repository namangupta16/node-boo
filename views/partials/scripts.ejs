            ${i?.mbti}
          </div>
        </div>`: ""}
        ${i?.enneagram?.key != "" ? `<div class="pe-2 pt-1">
          <div class="bubble personality-bubble ISTJ recommended-personality-bubble fw-normal px-3" style="background-color:#13A6A2; color:white">
            ${i?.enneagram?.key + `<img  style="height: 15px; margin: 1px;" src="/public/static/wing.png" alt="">` + i?.enneagram?.value}
          </div>
        </div>` : ""}
            ${i?.zodiac != "Zodiac" ?
          `<div class="pe-2 pt-1">
              <div class="bubble personality-bubble ISTJ recommended-personality-bubble fw-normal px-3" style="background-image: url('https://s3.us-west-2.amazonaws.com/secure.notion-static.com/e20215ad-e1bf-42d6-bf3f-d608fceb600a/space.png?X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Content-Sha256=UNSIGNED-PAYLOAD&X-Amz-Credential=AKIAT73L2G45EIPT3X45%2F20220903%2Fus-west-2%2Fs3%2Faws4_request&X-Amz-Date=20220903T190211Z&X-Amz-Expires=86400&X-Amz-Signature=aed8f7c7969b9b792e3b60a684a05f26c1469933b7ed8d7211f36804bf5d361d&X-Amz-SignedHeaders=host&response-content-disposition=filename%20%3D%22space.png%22&x-id=GetObject'); color:white;">
                ${i?.zodiac}
              </div>
            </div>` : ""
        }
          </div>
          <p style="line-height: 1.3;font-size: 14px;" class="my-3 fw-normal">
            ${i?.comment}
          </p>

          <div class="d-flex align-items-center ">
            <i 
              class="material-icons clickable like-button pe-1 hoverable hoverable-accent-color active-shrink-size-large text-muted"
              onclick="increaseLike(event, ${index})" style="color:${i?.like && '#4EDCD8 !important'}">favorite</i>
            <p  class="bodyText2 text-muted" > ${i?.likes}</p>
          </div>
        </div>
      `
    });
    document.getElementById("render-comments").innerHTML = "";
    document.getElementById("render-comments").appendChild(commentsDiv);
  }

  function increaseLike(event, index) {
    if (comments[index].like) {
      comments[index].likes = comments[index].likes - 1;
    } else {
      comments[index].likes = comments[index].likes + 1;
    }
    comments[index].like = !comments[index].like;
    renderComments(comments)
  }

