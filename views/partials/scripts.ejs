                    <% }); %>
                </div>
              </div>
              <!-- 3rd -->
              <div class="ml-2" style="opacity: 0.6"  id="text-zodiac-p">
                <div class="c-zodiac" onclick="document.getElementById('text-zodiac-full').style.display = 'flex'; document.getElementById('text-zodiac-p').style.opacity = 1;"
                  id="text-zodiac"
                  style="background-image: url('https://s3.us-west-2.amazonaws.com/secure.notion-static.com/e20215ad-e1bf-42d6-bf3f-d608fceb600a/space.png?X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Content-Sha256=UNSIGNED-PAYLOAD&X-Amz-Credential=AKIAT73L2G45EIPT3X45%2F20220903%2Fus-west-2%2Fs3%2Faws4_request&X-Amz-Date=20220903T190211Z&X-Amz-Expires=86400&X-Amz-Signature=aed8f7c7969b9b792e3b60a684a05f26c1469933b7ed8d7211f36804bf5d361d&X-Amz-SignedHeaders=host&response-content-disposition=filename%20%3D%22space.png%22&x-id=GetObject');">
