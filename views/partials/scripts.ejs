  let showLoader = false;
  function showLoaderFunction() {
    document.getElementById("load-loader").innerHTML = `<div id="loadingIndicator" class="position-fixed top-50 start-50 translate-middle"
    style="z-index: 100; padding-right: 5px; ">
    <div class="spinner-border accent-color-text" role="status"></div>
  </div>
    `;
  }

  function hideLoader() {
    document.getElementById("load-loader").innerHTML = ``;
  }

  function checkCanSubmitComment() {
    if ((document.getElementById('text-mbti').innerHTML?.trim() != 'MBTI' ||
      document.getElementById('text-enneagram').innerHTML?.trim() != 'Enneagram' ||
      document.getElementById('text-zodiac').innerHTML?.trim() != 'Zodiac')
      && (document.getElementById('title').value?.trim() != '' || document.getElementById('comment').value?.trim() != '')) {
      document.getElementById("comment-box-submit").disabled = false;
      document.getElementById("comment-box-submit-svg").style.fill = "#4EDCD8";
    } else {
      document.getElementById("comment-box-submit").disabled = true;
      document.getElementById("comment-box-submit-svg").style.fill = "";
    }
  }

  function settextmbti(value) {
    document.getElementById('text-mbti').innerHTML = value;
    checkCanSubmitComment();
  }

  function settextenneagram(key, value) {
    document.getElementById('text-enneagram').innerHTML = key + `<img  style="height: 15px; margin: 1px;" src="/public/static/wing.png" alt="">` + value;
    selectedEnneagram = { key, value }
    checkCanSubmitComment();
  }

  function settextzodiac(value) {
    document.getElementById('text-zodiac').innerHTML = value;
    checkCanSubmitComment();
  }

  function sortBy(type) {
    switch (type) {
      case 'best':
        document.getElementById('best-filter').className = "me-2 btn primaryBtn btn-primary";
        document.getElementById('best-filter').style.color = "white";
        document.getElementById('recent-filter').style.color = "black";
        document.getElementById('recent-filter').className = "me-2 btn primaryBtn btn-white";
        comments = comments.sort((a, b) => parseFloat(b.likes) - parseFloat(a.likes));
        break;

      case 'recent':
        document.getElementById('recent-filter').className = "me-2 btn primaryBtn btn-primary";
        document.getElementById('recent-filter').style.color = "white";
        document.getElementById('best-filter').style.color = "black";
        document.getElementById('best-filter').className = "me-2 btn primaryBtn btn-white";
        comments = comments.sort(function (a, b) {
          return new Date(b.createdDate) - new Date(a.createdDate)
        });
        break;
    }
    renderComments(comments);
  }

  let commentsTemp = comments;
  function filterBy(type) {
    switch (type) {
      case 'MBTI':
        comments = commentsTemp?.filter((i) => { return i.mbti != 'MBTI' });
        break;

      case 'Enneagram':
        comments = commentsTemp?.filter((i) => { return i.enneagram?.key != '' });
        break;

      case 'Zodiac':
        comments = commentsTemp?.filter((i) => { return i.zodiac != 'Zodiac' });
        break;

      default:
        comments = commentsTemp;
        break;
    }
    console.log("comments", comments);
    renderComments(comments);
  }

</script>